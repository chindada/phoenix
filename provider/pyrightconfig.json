{
  "include": ["."],
  "venvPath": "..",
  "venv": ".venv",
  "executionEnvironments": [
    {
      "root": "src"
    }
  ],
  "exclude": [
    "src/*_pb2.py",
    "src/*_pb2.pyi",
    "src/*_pb2_grpc.py",
    "**/__pycache__"
  ],
  "reportPropertyTypeMismatch": true,
  "reportReturnType": true,
  "reportArgumentType": true,
  "reportAttributeAccessIssue": true
}
